<Window x:Class="TumleTjek.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TumleTjek"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000"
        Background="#efded4"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,484,0,0" VerticalAlignment="Top" Height="44" Width="156" Background="#9f6e4e" Foreground="White" Command="{Binding ChildListButton}" CommandParameter="{Binding ElementName=mainframe}"/>
        <Frame Name="mainframe" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
