<UserControl x:Class="TumleTjek.View.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TumleTjek.View"
             mc:Ignorable="d" 
            
             Height="600" Width="1000"
             Background="#efded4"
             
             
             >
    <Grid>
        <Button Content="Tjek Ind" HorizontalAlignment="Center" Margin="0,516,0,0" VerticalAlignment="Top" Height="44" Width="156" Background="#9f6e4e" Foreground="White" Command="{Binding ChildListButton}"/>
        <Button Content="Medarbejder Login" HorizontalAlignment="Left" Margin="856,526,0,0" VerticalAlignment="Top" Height="34" Width="108" Background="#9f6e4e" Foreground="White" Command="{Binding MedarbejderLoginButton}"/>
        <Image x:Name="Logo" Source="/Image/LogoTumleTjek.png" Margin="127,28,85,109" />
        <TextBlock Text="Velkommen" HorizontalAlignment="Left" Margin="393,118,0,0" VerticalAlignment="Top" FontSize="45" FontWeight="Bold" Foreground="Black" Height="70" Width="256"/>

    </Grid>
</UserControl>
